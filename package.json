{
  "name": "@calebeby/preact-testing-library",
  "version": "0.0.4",
  "description": "",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "start": "tsc -w",
    "check-lint": "eslint --ignore-path .gitignore --ext .js,.ts . && prettier --list-different --ignore-path .gitignore '**/*.{j,t}s'",
    "lint": "eslint --ignore-path .gitignore --fix --ext .js,.ts . && prettier --write --ignore-path .gitignore '**/*.{j,t}s'"
  },
  "eslintConfig": {
    "extends": [
      "plugin:caleb/recommended"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@testing-library/dom": "^6.0.0"
  },
  "peerDependencies": {
    "preact": "^10.0.0-beta.1"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-plugin-caleb": "^5.0.0",
    "preact": "^10.0.0-rc.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
